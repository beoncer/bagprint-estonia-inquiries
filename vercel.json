{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist-prerendered",
  "installCommand": "npm install",
  "framework": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/kontakt",
      "dest": "/kontakt.html"
    },
    {
      "src": "/meist",
      "dest": "/meist.html"
    },
    {
      "src": "/portfoolio",
      "dest": "/portfoolio.html"
    },
    {
      "src": "/blogi",
      "dest": "/blogi.html"
    },
    {
      "src": "/privaatsus",
      "dest": "/privaatsus.html"
    },
    {
      "src": "/ostutingimused",
      "dest": "/ostutingimused.html"
    },
    {
      "src": "/tooted",
      "dest": "/tooted.html"
    },
    {
      "src": "/riidest-kotid",
      "dest": "/riidest-kotid.html"
    },
    {
      "src": "/paberkotid",
      "dest": "/paberkotid.html"
    },
    {
      "src": "/nooriga-kotid",
      "dest": "/nooriga-kotid.html"
    },
    {
      "src": "/sussikotid",
      "dest": "/sussikotid.html"
    },
    {
      "src": "/e-poe-pakendid",
      "dest": "/e-poe-pakendid.html"
    },
    {
      "src": "/tooted/(.*)",
      "dest": "/tooted/$1.html"
    },
    {
      "src": "/blogi/(.*)",
      "dest": "/blogi/$1.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/kontakt/",
      "destination": "/kontakt",
      "permanent": true
    },
    {
      "source": "/meist/",
      "destination": "/meist",
      "permanent": true
    },
    {
      "source": "/portfoolio/",
      "destination": "/portfoolio",
      "permanent": true
    },
    {
      "source": "/blogi/",
      "destination": "/blogi",
      "permanent": true
    },
    {
      "source": "/privaatsus/",
      "destination": "/privaatsus",
      "permanent": true
    },
    {
      "source": "/ostutingimused/",
      "destination": "/ostutingimused",
      "permanent": true
    },
    {
      "source": "/tooted/",
      "destination": "/tooted",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}