{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist-prerendered",
  "installCommand": "npm install",
  "framework": "vite",
  "routes": [
    {
      "src": "/kontakt",
      "dest": "/kontakt.html"
    },
    {
      "src": "/meist",
      "dest": "/meist.html"
    },
    {
      "src": "/portfoolio",
      "dest": "/portfoolio.html"
    },
    {
      "src": "/blogi",
      "dest": "/blogi.html"
    },
    {
      "src": "/privaatsus",
      "dest": "/privaatsus.html"
    },
    {
      "src": "/ostutingimused",
      "dest": "/ostutingimused.html"
    },
    {
      "src": "/tooted",
      "dest": "/tooted.html"
    },
    {
      "src": "/riidest-kotid",
      "dest": "/riidest-kotid.html"
    },
    {
      "src": "/paberkotid",
      "dest": "/paberkotid.html"
    },
    {
      "src": "/nooriga-kotid",
      "dest": "/nooriga-kotid.html"
    },
    {
      "src": "/sussikotid",
      "dest": "/sussikotid.html"
    },
    {
      "src": "/e-poe-pakendid",
      "dest": "/e-poe-pakendid.html"
    },
    {
      "src": "/tooted/(.*)",
      "dest": "/tooted/$1.html"
    },
    {
      "src": "/blogi/(.*)",
      "dest": "/blogi/$1.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "no-referrer-when-downgrade"
        }
      ]
    }
  ]
}