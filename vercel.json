{
  "functions": {
    "api/ssr.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    { "src": "^/sitemap.xml", "dest": "https://ixotpxliaerkzjznyipi.supabase.co/functions/v1/sitemap" },
    { "src": "^/admin/(.*)", "dest": "/index.html" },
    { "src": "^/assets/(.*)", "dest": "/assets/$1" },
    { "src": "^/(.*)\\.(js|css|map|png|jpg|webp|svg|ico)$", "dest": "/$1" },
    { "src": "^(?!/api/).*", "dest": "/api/ssr" },
    {
      "src": "/admin/(.*)",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml",
        "Cache-Control": "public, max-age=3600, s-maxage=3600"
      }
    }
  ]
}