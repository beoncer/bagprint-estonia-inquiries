{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist-prerendered",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/kontakt",
      "destination": "/kontakt.html"
    },
    {
      "source": "/meist",
      "destination": "/meist.html"
    },
    {
      "source": "/portfoolio",
      "destination": "/portfoolio.html"
    },
    {
      "source": "/blogi",
      "destination": "/blogi.html"
    },
    {
      "source": "/privaatsus",
      "destination": "/privaatsus.html"
    },
    {
      "source": "/ostutingimused",
      "destination": "/ostutingimused.html"
    },
    {
      "source": "/tooted",
      "destination": "/tooted.html"
    },
    {
      "source": "/riidest-kotid",
      "destination": "/riidest-kotid.html"
    },
    {
      "source": "/paberkotid",
      "destination": "/paberkotid.html"
    },
    {
      "source": "/nooriga-kotid",
      "destination": "/nooriga-kotid.html"
    },
    {
      "source": "/sussikotid",
      "destination": "/sussikotid.html"
    },
    {
      "source": "/e-poe-pakendid",
      "destination": "/e-poe-pakendid.html"
    },
    {
      "source": "/tooted/:slug",
      "destination": "/tooted/:slug.html"
    },
    {
      "source": "/blogi/:slug",
      "destination": "/blogi/:slug.html"
    }
  ]
}