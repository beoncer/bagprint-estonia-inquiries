{
  "functions": {
    "api/ssr.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/sitemap.xml",
      "dest": "https://ixotpxliaerkzjznyipi.supabase.co/functions/v1/sitemap"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/(.*)\\.(js|css|map|png|jpg|jpeg|gif|webp|svg|ico|woff|woff2|ttf|eot)$",
      "dest": "/$1"
    },
    {
      "src": "^/sw\\.js$",
      "dest": "/sw.js"
    },
    {
      "src": "^/manifest\\.json$",
      "dest": "/manifest.json"
    },
    {
      "src": "^/robots\\.txt$",
      "dest": "/robots.txt"
    },
    {
      "src": "^(?!/api/).*",
      "dest": "/api/ssr"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        }
      ]
    }
  ]
}