{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist-prerendered",
  "installCommand": "npm install",
  "framework": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/kontakt",
      "destination": "/kontakt/index.html"
    },
    {
      "source": "/meist",
      "destination": "/meist/index.html"
    },
    {
      "source": "/portfoolio",
      "destination": "/portfoolio/index.html"
    },
    {
      "source": "/blogi",
      "destination": "/blogi/index.html"
    },
    {
      "source": "/privaatsus",
      "destination": "/privaatsus/index.html"
    },
    {
      "source": "/ostutingimused",
      "destination": "/ostutingimused/index.html"
    },
    {
      "source": "/tooted",
      "destination": "/tooted/index.html"
    },
    {
      "source": "/riidest-kotid",
      "destination": "/riidest-kotid/index.html"
    },
    {
      "source": "/paberkotid",
      "destination": "/paberkotid/index.html"
    },
    {
      "source": "/nooriga-kotid",
      "destination": "/nooriga-kotid/index.html"
    },
    {
      "source": "/sussikotid",
      "destination": "/sussikotid/index.html"
    },
    {
      "source": "/e-poe-pakendid",
      "destination": "/e-poe-pakendid/index.html"
    },
    {
      "source": "/tooted/:slug",
      "destination": "/tooted/:slug/index.html"
    },
    {
      "source": "/blogi/:slug",
      "destination": "/blogi/:slug/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "no-referrer-when-downgrade"
        }
      ]
    }
  ]
}