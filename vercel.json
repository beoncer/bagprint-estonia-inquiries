{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist-prerendered",
  "installCommand": "npm install",
  "framework": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/kontakt",
      "destination": "/kontakt.html"
    },
    {
      "source": "/meist",
      "destination": "/meist.html"
    },
    {
      "source": "/portfoolio",
      "destination": "/portfoolio.html"
    },
    {
      "source": "/blogi",
      "destination": "/blogi.html"
    },
    {
      "source": "/privaatsus",
      "destination": "/privaatsus.html"
    },
    {
      "source": "/ostutingimused",
      "destination": "/ostutingimused.html"
    },
    {
      "source": "/tooted",
      "destination": "/tooted.html"
    },
    {
      "source": "/riidest-kotid",
      "destination": "/riidest-kotid.html"
    },
    {
      "source": "/paberkotid",
      "destination": "/paberkotid.html"
    },
    {
      "source": "/nooriga-kotid",
      "destination": "/nooriga-kotid.html"
    },
    {
      "source": "/sussikotid",
      "destination": "/sussikotid.html"
    },
    {
      "source": "/e-poe-pakendid",
      "destination": "/e-poe-pakendid.html"
    },
    {
      "source": "/tooted/:slug",
      "destination": "/tooted/:slug.html"
    },
    {
      "source": "/blogi/:slug",
      "destination": "/blogi/:slug.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/kontakt/",
      "destination": "/kontakt",
      "permanent": true
    },
    {
      "source": "/meist/",
      "destination": "/meist",
      "permanent": true
    },
    {
      "source": "/portfoolio/",
      "destination": "/portfoolio",
      "permanent": true
    },
    {
      "source": "/blogi/",
      "destination": "/blogi",
      "permanent": true
    },
    {
      "source": "/privaatsus/",
      "destination": "/privaatsus",
      "permanent": true
    },
    {
      "source": "/ostutingimused/",
      "destination": "/ostutingimused",
      "permanent": true
    },
    {
      "source": "/tooted/",
      "destination": "/tooted",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}