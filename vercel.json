{
  "functions": {
    "scripts/generate-seo-pages.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "https://ixotpxliaerkzjznyipi.supabase.co/functions/v1/sitemap"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/$",
      "destination": "/seo-index.html"
    },
    {
      "source": "/tooted$",
      "destination": "/tooted/index.html"
    },
    {
      "source": "/riidest-kotid$",
      "destination": "/riidest-kotid/index.html"
    },
    {
      "source": "/paberkotid$",
      "destination": "/paberkotid/index.html"
    },
    {
      "source": "/nooriga-kotid$",
      "destination": "/nooriga-kotid/index.html"
    },
    {
      "source": "/sussikotid$",
      "destination": "/sussikotid/index.html"
    },
    {
      "source": "/kontakt$",
      "destination": "/kontakt/index.html"
    },
    {
      "source": "/meist$",
      "destination": "/meist/index.html"
    },
    {
      "source": "/portfoolio$",
      "destination": "/portfoolio/index.html"
    },
    {
      "source": "/blogi$",
      "destination": "/blogi/index.html"
    },
    {
      "source": "/tooted/([^/]+)$",
      "destination": "/tooted/$1/index.html"
    },
    {
      "source": "/blogi/([^/]+)$",
      "destination": "/blogi/$1/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/admin/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        }
      ]
    }
  ]
}